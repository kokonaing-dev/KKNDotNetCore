<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - KKNDotNetCore.MvcChartApp</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/KKNDotNetCore.MvcChartApp.styles.css" asp-append-version="true" />
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container-fluid">
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">KKNDotNetCore.MvcChartApp</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
                        </li>
                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle" href="#" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                                Chart
                            </a>
                            <ul class="dropdown-menu">
                                <li><a class="dropdown-item" href="/ApexChart/SimplePieChart">ApexChart - SimplePieChart</a></li>
                                <li><a class="dropdown-item" href="/ApexChart/DumbbellChart">ApexChart - DumbbellChart</a></li>
                                <li><a class="dropdown-item" href="/HighChart/PieChart">HighChart - PieChart</a></li>
                                <li><a class="dropdown-item" href="/HighChart/LineChart">HighChart - LineChart</a></li>
                                <li><a class="dropdown-item" href="/CanvasJs/LineChart">CanvasJs - LineChart</a></li>
                                <li><a class="dropdown-item" href="/CanvasJs/ScatterChart">CanvasJs - ScatterChart</a></li>
                                <li><a class="dropdown-item" href="/ChartJs/ExampleChart">ChartJs - ExampleChart</a></li>
                                <li><a class="dropdown-item" href="/ChartJs/InterpolationLineChart">ChartJs - InterpolationLineChart</a></li>
                                <li><a class="dropdown-item" href="/ChartJs/PolarAreaChart">ChartJs - PolarAreaChart</a></li>
                            </ul>
                        </li>

                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2024 - KKNDotNetCore.MvcChartApp - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="https://cdn.jsdelivr.net/npm/apexcharts"></script>
    <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
    <script src="https://code.highcharts.com/highcharts.js"></script>
    <script src="https://cdn.canvasjs.com/canvasjs.min.js"></script>

    @await RenderSectionAsync("scripts", required: false)
</body>
</html>
